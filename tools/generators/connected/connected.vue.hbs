<template>
  <div>
    <h1>{{ properCase componentName }}</h1>
    <div v-for="item in {{ camelCase moduleName }}">
      <p>\{{ item.title }}</p>
    </div>
  </div>
</template>

<script lang="ts">
  import { mapGetters } from 'vuex';
  import { IPreLoad } from '../../../server/isomorphic';

  export default {
    name: '{{ properCase componentName }}',
    components: {},
    props: {},
    data(): any {
      return {};
    },
    computed: {
      ...mapGetters({
        {{ camelCase moduleName }}: 'get{{ properCase moduleName }}'
      }),
    },
    methods: {},
    prefetch: (options: IPreLoad) => {
      return options.store.dispatch(`get{{ properCase moduleName }}`);
    },
  };
</script>

