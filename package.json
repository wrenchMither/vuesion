{
  "name": "vuesion",
  "private": true,
  "version": "3.7.2",
  "description": "The most complete boilerplate for production-ready PWAs. With focus on performance, development speed, and best practices",
  "repository": {
    "type": "git",
    "url": "https://github.com/vuesion/vuesion.git"
  },
  "keywords": [
    "vue",
    "vuex",
    "vue-router",
    "webpack",
    "typescript",
    "server-side-rendering",
    "boilerplate",
    "jest",
    "enterprise",
    "components",
    "design-system",
    "nuxt.js",
    "nuxt"
  ],
  "author": "Johannes Werner <johannes.werner86@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/vuesion/vuesion",
  "bugs": {
    "url": "https://github.com/vuesion/vuesion/issues"
  },
  "scripts": {
    "dev": "nuxt-ts",
    "generate": "vuesion generate",
    "add": "vuesion add",
    "extract-i18n-messages": "vuesion extract-i18n-messages",
    "test": "vuesion test --coverage",
    "test:watch": "vuesion test --coverage --watch",
    "e2e": "vuesion e2e",
    "lint": "vuesion lint --cache",
    "clean": "vuesion clean",
    "storybook:dev": "vuesion storybook --dev",
    "storybook:build": "vuesion storybook",
    "update": "vuesion update",
    "prettier": "vuesion prettier",
    "release:major": "vuesion release --major",
    "release:minor": "vuesion release --minor",
    "release:patch": "vuesion release --patch",
    "build": "nuxt-ts build",
    "build:analyze": "nuxt-ts build --analyze",
    "build:spa": "nuxt-ts generate",
    "start": "nuxt-ts start",
    "e2e:ci": "npx start-server-and-test dev http://localhost:3000 e2e",
    "ci": "vuesion parallel \"npm run lint\" \"npm run test -- --runInBand\" \"npm run e2e:ci\"",
    "postci": "vuesion parallel \"npm run build\" \"npm run storybook:build\""
  },
  "lint-staged": {
    "*.{js,ts,vue}": "npm run lint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "vuesion parallel \"vuesion prettier --staged\" \"npm run lint\" \"npm run test\""
    }
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "0.4.10",
    "@nuxtjs/axios": "5.12.2",
    "@nuxtjs/pwa": "3.0.2",
    "@types/animejs": "3.1.2",
    "@types/cookie-parser": "1.4.2",
    "@types/helmet": "0.0.48",
    "@vue/composition-api": "0.6.7",
    "@vuesion/utils": "4.0.0-alpha.12",
    "@vuesion/vuex-persist": "4.0.0-alpha.12",
    "animejs": "3.2.0",
    "compression": "1.7.4",
    "config": "3.3.1",
    "cookie-parser": "1.4.5",
    "helmet": "3.23.3",
    "lodash": "4.17.20",
    "marked": "1.1.1",
    "nuxt": "2.14.5",
    "nuxt-i18n": "6.15.1",
    "query-string": "6.13.1",
    "vee-validate": "3.3.11"
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.2",
    "@nuxt/typescript-build": "1.0.3",
    "@nuxtjs/eslint-config-typescript": "2.1.0",
    "@nuxtjs/eslint-module": "2.0.0",
    "@storybook/addon-actions": "5.3.21",
    "@storybook/vue": "5.3.21",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/vue": "5.0.4",
    "@types/jest": "26.0.13",
    "@types/lodash": "4.14.161",
    "@types/marked": "1.1.0",
    "@types/testing-library__jest-dom": "5.9.2",
    "@vue/test-utils": "1.1.0",
    "@vuesion/service": "4.0.0-alpha.12",
    "@vuesion/storybook-theme-switcher": "4.0.0-alpha.12",
    "axios-mock-adapter": "1.18.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.3.0",
    "eslint": "7.8.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "3.1.4",
    "flush-promises": "1.0.2",
    "husky": "4.3.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.4.2",
    "lint-staged": "10.3.0",
    "node-sass": "4.14.1",
    "prettier": "2.1.1",
    "sass": "1.26.10",
    "sass-loader": "8.0.2",
    "ts-jest": "26.3.0",
    "vue-jest": "3.0.6"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.0.0"
  },
  "jest": {
    "globals": {
      "window": true,
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
    },
    "rootDir": ".",
    "setupFiles": [
      "<rootDir>/src/test/jestsetup.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/test/jest-dom-importer.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^vue$": "vue/dist/vue.common.js"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/cypress/"
    ],
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/layouts/",
      "/plugins/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 99,
        "functions": 99,
        "lines": 100
      }
    },
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "html",
      "text",
      "lcov",
      "json"
    ],
    "preset": "ts-jest",
    "testMatch": null
  }
}
